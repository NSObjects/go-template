# 环境变量配置示例
# 复制此文件为 .env 并修改相应值

# 系统配置
RUN_ENVIRONMENT=dev
SYSTEM_PORT=:9322
SYSTEM_LEVEL=1
SYSTEM_ENV=dev

# 数据库配置
MYSQL_HOST=127.0.0.1
MYSQL_PORT=3306
MYSQL_DATABASE=test
MYSQL_USER=root
MYSQL_PASSWORD=12345678
MYSQL_MAX_IDLE_CONNS=20
MYSQL_MAX_OPEN_CONNS=20

# Redis配置
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DATABASE=0

# JWT配置
JWT_SECRET=tn)M^P<j,/6$Gr/Wrs
JWT_EXPIRE=3600

# 日志配置
LOG_LEVEL=debug
LOG_FORMAT=color

# CORS配置
CORS_ALLOW_ORIGINS=*
CORS_ALLOW_HEADERS=Origin,Content-Type,Accept
CORS_ALLOW_METHODS=GET,HEAD,PUT,PATCH,POST,DELETE
CORS_ALLOW_CREDENTIALS=true
